{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Sensible",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab", "contextMenus", "storage", "identity"
    ]
}